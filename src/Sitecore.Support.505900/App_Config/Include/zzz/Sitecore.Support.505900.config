<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
      <pipelines>
          <group groupName="ExperienceProfileContactViews">
              <pipelines>
                  <events>
                      <processor type="Sitecore.Cintel.Reporting.Processors.EventsReportProcessor, Sitecore.Cintel">
                          <patch:attribute name="type">Sitecore.Support.Cintel.Reporting.Processors.EventsReportProcessor, Sitecore.Support.505900</patch:attribute>
                      </processor>
                  </events>                  
              </pipelines>
          </group>         
       </pipelines>
      <settings>
          <setting name="PathAnalyzer.Events.NoSite" value="No site info" />
          <setting name="PathAnalyzer.Events.NoUrl" value="no url" />
      </settings>
  </sitecore>
</configuration>